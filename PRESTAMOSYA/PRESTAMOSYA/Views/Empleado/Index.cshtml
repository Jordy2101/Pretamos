@model IEnumerable<PRESTAMOSYA.Models.Empleado>

@{
    ViewBag.Title = "Index";
}


<br />
<div class="jumbotron">
    <h1 class="display-2"><center>Registro de Empleado</center></h1>
</div>

<nav aria-label="breadcrumb">
    <ol class="breadcrumb">
        <li class="breadcrumb-item " aria-current="page">
            <a class="btn btn-default" href="~/Empleado/Create">Crear nuevo Empleado</a>
        </li>
    </ol>
</nav>


<table class="table">
    <tr>
        <th style="width:200px;">
            @*@Html.DisplayNameFor(model => model.Nombre_Empleado)*@
            Nombre de Empleado
        </th>
        <th style="width:200px;">
            @*@Html.DisplayNameFor(model => model.Cedula_Empleado)*@
            Cedula
        </th>
        <th style="width:200px;">
            @Html.DisplayNameFor(model => model.Cargo)
        </th>
        <th style="width:200px;">
            @Html.DisplayNameFor(model => model.Contraseña)
        </th>
        <th style="width:200px;"></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Nombre_Empleado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cedula_Empleado)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Cargo)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Contraseña)
            </td>
            <td>
          
                <a href="~/Empleado/Edit/@item.ID_Empleado" class="btn btn-success"><i class="glyphicon glyphicon-edit"></i></a>
                <a href="~/Empleado/Details/@item.ID_Empleado" class="btn btn-info "><i class="glyphicon glyphicon-info-sign "></i> </a>
                <a href="~/Empleado/Delete/@item.ID_Empleado" class=" btn btn-danger"><i class=" glyphicon glyphicon-trash" </a>

            </td>
        </tr>
    }

</table>
